{
  "name": "mhsaheb/laravel-rpc-rabbitmq",
  "description": "Request/Reply (RPC) over RabbitMQ for Laravel, using direct-reply-to.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "php-amqplib/php-amqplib": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "RpcRabbit\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RpcRabbit\\RpcServiceProvider"
      ],
      "aliases": {
        "Rpc": "RpcRabbit\\Facades\\Rpc"
      }
    }
  }
}